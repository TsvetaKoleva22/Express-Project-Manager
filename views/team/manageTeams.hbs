<h1>Teams</h1>
<h3>Manage Users</h3>
<div class="form">
    <form action="/team/manage" method="POST">
        <label>User</label>
        <select name="userSelected">
            {{#each usersArr}}
            <option value="{{_id}}">{{username}}</option>
            {{/each}}
        </select>
        <label>Team</label>
        <select name="teamSelected">
            {{#each teamsArr}}
            <option value="{{_id}}">{{name}}</option>
            {{/each}}
        </select>
        <input type="submit" value="Distribute">
    </form>
</div>